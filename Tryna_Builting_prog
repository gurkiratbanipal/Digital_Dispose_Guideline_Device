#include "smarttrash.h"

namespace Win_Forms_demo
{

	Form1::Form1()
	{
		InitializeComponent();
	}

	void Form1::label1_Click(std::any sender, EventArgs *e)
	{
		std::cout << "Smart Trash Book" << std::endl;
	}

	void Form1::button2_Click(std::any sender, EventArgs *e)
	{
		MessageBox::Show("You are almost there!", "Lets Start Learning ;-)");
	}

	void Form1::button3_Click(std::any sender, EventArgs *e)
	{
		MessageBox::Show("Thank You", "YES");
	}

	void Form1::button4_Click(std::any sender, EventArgs *e)
	{
		MessageBox::Show(L"Suggestion", "NO");
	}

	void Form1::label2_Click(std::any sender, EventArgs *e)
	{
		std::cout << "Reviews" << std::endl;
	}

	void Form1::button1_Click(std::any sender, EventArgs *e)
	{

		Application::Exit();
	}
}
